{
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 4,
    "printWidth": 100,
    "endOfLine": "auto",
    "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
